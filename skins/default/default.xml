<skin width="800" height="600" background="background.png" header_space="64">
	<font family="Calibri" size="12"/>
	<panel
	height="79"
	position="bottom"
	padding-left="60"
	padding-right="60"
	background="mbg.png">
	</panel>

	<modules>
		<default_home>
			<button
			released="button_up.png"
			pressed="button_down.png"/>

			<buttons>
			<button name="mp3player" value="MP3 Player"
				appear-pause="0"
				disappear-pause="0"
				released="mp3_btn_up.png"
				pressed="mp3_btn_down.png">

				<rect x="20" y="20" width="276" height="94"/>
				<animation velocity="1" time="20">
				    	<stop time="0" x="-276" y="20" width="276" height="94" opacity="1" acceleration="1"/>
					<stop time="15" x="10" y="20" width="276" height="94" opacity="1" acceleration="1"/>
					<stop time="20" x="20" y="20" width="276" height="94" opacity="1" acceleration="1"/>
				</animation>
			</button>
			<button name="phone" value="Phone"
				appear-pause="0"
				disappear-pause="0"
				released="video_btn_up.png"
				pressed="video_btn_down.png">

				<rect x="20" y="140" width="276" height="94"/>
				<animation velocity="1" time="30">
				    	<stop time="0" x="-276" y="140" width="276" height="94" opacity="1" acceleration="1"/>
				    	<stop time="10" x="-276" y="140" width="276" height="94" opacity="1" acceleration="1"/>
					<stop time="25" x="10" y="140" width="276" height="94" opacity="1" acceleration="1"/>
					<stop time="30" x="20" y="140" width="276" height="94" opacity="1" acceleration="1"/>
				</animation>
			</button>
			<button name="gps" value="GPS"
				appear-pause="0"
				disappear-pause="0"
				released="gps_btn_up.png"
				pressed="gps_btn_down.png">

				<rect x="20" y="260" width="276" height="94"/>
				<animation velocity="1" time="40">
				    	<stop time="0" x="-276" y="260" width="276" height="94" opacity="1" acceleration="1"/>
				    	<stop time="20" x="-276" y="260" width="276" height="94" opacity="1" acceleration="1"/>
					<stop time="35" x="10" y="260" width="276" height="94" opacity="1" acceleration="1"/>
					<stop time="40" x="20" y="260" width="276" height="94" opacity="1" acceleration="1"/>
				</animation>
			</button>

 			</buttons>
		</default_home>

		<volctl>
			<panel background="volctl_panel_bg.png"/>
			<volume_up_button
			released="volctl_volume_up.png"
			pressed="volctl_volume_up_act.png"/>

			<volume_down_button
			released="volctl_volume_down.png"
			pressed="volctl_volume_down_act.png"/>

			<volume_mute_button
			released="volctl_mute.png"
			pressed="volctl_mute_act.png"/>
		</volctl>

		<mp3player>
			<display
			background="display.png"
			font-family="Calibry"
			font-size="18">
				<rect x="10" y="10" width="432" height="190"/>
			        <animation velocity="1" time="10">
					<stop time="0" x="-400" y="10" width="432" height="190" opacity="1" acceleration="1"/>
					<stop time="10" x="10" y="10" width="432" height="190" opacity="1" acceleration="1"/>
				</animation>
			</display>
			<list name="playlist">
				<rect x="450" y="10" width="330" height="390"/>
				<!--border images for list  -->
				<borders
					upper-left="list_ul.png"
					upper-right="list_ur.png"
					top="list_u.png"
					bottom-left="list_bl.png"
					bottom-right="list_br.png"
					bottom="list_b.png"
					left="list_l.png"
					right="list_r.png"
				/>
				<selector image="list_selector.png" fill="list_selector_fill.png"/>
			</list>
			<button name="first"
				released="first_btn_up.png"
				pressed="first_btn_up.png">
				<rect x="10" y="260" width="81" height="91"/>
			</button>
			<button name="back"
				released="back_btn_up.png"
				pressed="back_btn_up.png">
				<rect x="88" y="260" width="81" height="91"/>
			</button>
			<button name="play"
				released="play_btn_up.png"
				pressed="play_btn_up.png">
				<rect x="153" y="227" width="139" height="160"/>
			</button>

			<button name="stop"
				released="stop_btn_up.png"
				pressed="stop_btn_up.png">
				<rect x="153" y="227" width="139" height="160"/>
			</button>

			<button name="next"
				released="next_btn_up.png"
				pressed="next_btn_up.png">
				<rect x="275" y="260" width="81" height="91"/>
			</button>
			<button name="last"
				released="last_btn_up.png"
				pressed="last_btn_up.png">
				<rect x="353" y="260" width="81" height="91"/>
			</button>
			<button name="device"
				released="buttonbar_left_up.png"
				pressed="buttonbar_left_up.png">
				<rect x="60" y="410" width="116" height="36"/>
			</button>
			<button name="settings"
				released="buttonbar_mid_up.png"
				pressed="buttonbar_mid_up.png">
				<rect x="176" y="410" width="116" height="36"/>
			</button>
			<button name="test"
				released="buttonbar_right_up.png"
				pressed="buttonbar_right_up.png">
				<rect x="292" y="410" width="116" height="36"/>
			</button>

			<button name="repeat"
				released="buttonbar_left_up.png"
				pressed="buttonbar_left_up.png">
				<rect x="488" y="410" width="116" height="36"/>
			</button>
			<button name="display"
				released="buttonbar_right_up.png"
				pressed="buttonbar_right_up.png">
				<rect x="604" y="410" width="116" height="36"/>
			</button>
		</mp3player>
	</modules>
	<home module="default_home">
	</home>
</skin>
