<?xml version="1.0"?>
<config>
    <buttons>
	<button name="MP3" value="MP3 Player"
	    appear-pause="0"
	    disappear-pause="0">

	    <passive-image src="btn1_up.png"/>
	    <active-image src="btn1_down.png"/>
	    <position x="10" y="10" width="100" height="100"/>

	    <animation type="flow" velocity="1">
	    	<stop time="0" x="0" y="0" width="0" height="0" opacity="1" acceleration="1"/>
		<stop time="5" x="20" y="20" width="100" height="100" opacity="1" acceleration="3"/>
		<stop time="10" x="10" y="10" width="100" height="100" opacity="1" acceleration="1"/>
	    </animation>
	</button>
    </buttons>
</config>
