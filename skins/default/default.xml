<skin resolution="800x600" background="background.png">
	<font family="Calibri"/>
	<panel
	height="79"
	position="bottom"
	padding-left="60"
	padding-right="60"
	background="mbg.png">
	</panel>

	<modules>
		<volctl>
			<panel background="volctl_panel_bg.png"/>
			<volume_up_button
			released="volctl_volume_up.png"
			pressed="volctl_volume_up_act.png"/>

			<volume_down_button
			released="volctl_volume_down.png"
			pressed="volctl_volume_down_act.png"/>

			<volume_mute_button
			released="volctl_mute.png"
			pressed="volctl_mute_act.png"/>
		</volctl>
	</modules>
</skin>